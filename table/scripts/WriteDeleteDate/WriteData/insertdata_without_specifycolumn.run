--1. 创建数据库
create database test_db;

--2. 指定数据库
use test_db;

--3. 创建表
create table table_1
(
   	device_id STRING TAG,
    model_id STRING ATTRIBUTE,
	boolean BOOLEAN FIELD,
	int32 INT32 FIELD,
	int64 INT64 FIELD,
	float FLOAT FIELD,
	double DOUBLE FIELD,
	text TEXT FIELD,
	string STRING FIELD,
	blob BLOB FIELD,
	timestamp TIMESTAMP FIELD,
	date DATE FIELD
);

--4. 查看表
show tables;

--5. 展示表结构信息
desc table_1;

