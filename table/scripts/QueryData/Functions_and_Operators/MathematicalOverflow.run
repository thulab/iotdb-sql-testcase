// 部分数据类型进行数学运行，溢出校验

connect root/TimechoDB@2021;
--0.清理数据: 删除数据库 
drop database test_db;
<<NULL;


--1. 创建数据库
create database test_db;

--2. 指定数据库
use test_db;

--3. 创建表
create table table_add
(
    "device" STRING TAG,
    "type" STRING ATTRIBUTE,
    "int32" INT32,
	"int32_1" INT32,
    "int64" INT64,
    "double" DOUBLE,
    "float" FLOAT,
    "timestamp" TIMESTAMP,
    "date" DATE
);

create table table_sub
(
    "device" STRING TAG,
    "type" STRING ATTRIBUTE,
    "int32" INT32,
	"int32_1" INT32,
    "int64" INT64,
	"int64_1" INT64,
    "double" DOUBLE,
    "float" FLOAT,
    "timestamp" TIMESTAMP,
    "date" DATE
);

create table table_mul
(
    "device" STRING TAG,
    "type" STRING ATTRIBUTE,
    "int32" INT32,
	"int32_1" INT32,
    "int64" INT64,
	"int64_1" INT64,
    "double" DOUBLE,
    "float" FLOAT
);

create table table_div
(
    "device" STRING TAG,
    "type" STRING ATTRIBUTE,
    "int32" INT32,
	"int32_1" INT32,
    "int64" INT64,
	"int64_1" INT64,
    "double" DOUBLE,
    "float" FLOAT
);

create table table_negation
(
    "device" STRING TAG,
    "type" STRING ATTRIBUTE,
    "int32" INT32,
    "int64" INT64,
    "double" DOUBLE,
    "float" FLOAT,
	"timestamp" TIMESTAMP
);

