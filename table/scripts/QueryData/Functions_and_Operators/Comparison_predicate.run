// 比较谓词

connect root/TimechoDB@2021;
--0.清理数据: 删除数据库 
drop database test_db;
<<NULL;


--1. 创建数据库
create database test_db;


--2. 指定数据库
use test_db;


--3. 创建表 t1
create table t1
(
    "device" STRING TAG,
    "type" STRING ATTRIBUTE,
    "int32" INT32,
    "int64" INT64,
    "double" DOUBLE,
    "float" FLOAT,
    "boolean" BOOLEAN,
    "text" TEXT,
    "string" STRING,
    "blob" BLOB,
    "timestamp" TIMESTAMP,
    "date" DATE
);


