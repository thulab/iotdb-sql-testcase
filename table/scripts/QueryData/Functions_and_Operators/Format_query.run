select format('%s, %s, %s, %f, %d, %tY, %f, %tH, %s, %d, %s, %s',tag1, attr1, s1, s2, s3, s4, s5, s6, s7, s8, s9, s10) from test_t1 order by time;
select format('%s, %1$d',s3) from test_t1 order by time;

